---
import Header from '$components/saweria/Header.astro'
import Navbar from '$components/saweria/Navbar.astro'
import Card from '$components/saweria/Card.astro'
import CardWithTap from '$components/saweria/CardWithTap.astro'
import Layout from '../layouts/SeedBKK.astro'
import Footer from '$components/Footer.astro'
import ContainerCards from '@/components/saweria/ContainerCards.astro'

const msg = `เราคือชุมชนคริสเตียน คริสเตียนคือคนที่เชื่อ รัก และติดตามพระเยซูคริสต์ เราเชื่อว่าพระเจ้าเป็นผู้สร้างทุกสิ่งรวมถึงชีวิตของคุณด้วย หากคุณสนใจที่จะรู้จักเรื่องราวของพระเจ้าเพิ่มเติม เรายินดีที่ให้ข้อมูลเพื่อที่คุณจะได้รู้จักเรื่องราวพระเจ้ามากขึ้น`
---

<Layout>
  <Navbar />
  <Header />

  <div class="custom-content container">
    <div class="col-md-12 col-sm-12 col-12">
      <div class="card">
        <div class="card-body">
          <p class="card-text">
            QRCODE WEB
            <br />
            <img src="/icons/seedchbkk.svg" width={350} height={350} />
          </p>
        </div>
      </div>
    </div>
  </div>
  <br />

  <ContainerCards />
  <br />
  <Card body={msg} />
  <br />

  <Footer />
</Layout>
