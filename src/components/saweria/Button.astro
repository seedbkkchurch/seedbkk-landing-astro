---
interface Props {
    title: string
    body: string
}

const { title, body } = Astro.props
---

<div class="custom-content container">
    <div class="col-md-12 col-sm-12 col-12">
        <div class="card">
            <div class="card-body">
                {title}
                <p class="card-text">
                    {body}
                </p>
            </div>
        </div>
    </div>
</div>
